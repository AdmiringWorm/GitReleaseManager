<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <LangVersion>8.0</LangVersion>
        <TargetFrameworks>net472;netcoreapp3.0</TargetFrameworks>
        <Title>GitReleaseManager.IntegrationTests</Title>
        <Description>Integration Test Project for GitReleaseManager</Description>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <NoWarn>CA1707</NoWarn>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="..\SolutionInfo.cs" Link="SolutionInfo.cs" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\GitReleaseManager\GitReleaseManager.Core.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="nunit" Version="3.12.0" />
        <PackageReference Include="NUnit3TestAdapter" Version="3.15.1" />
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.2.0"/>
        <PackageReference Include="TextCopy" Version="1.7.1" />
    </ItemGroup>
</Project>